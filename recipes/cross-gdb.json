{
    "name": "cross-gdb",
    "description": "Cross-compiler used to link packages.",
    "url": "https://mirror.csclub.uwaterloo.ca/gnu/gdb/gdb-16.3.tar.xz",
    "version": [ 16,3,0 ],
    "depends": [],
    "patches": [
        {
            "patch": "patches/binutils/config.bfd.patch",
            "modifies": "gdb-16.3/bfd/config.bfd"
        },
        {
            "patch": "patches/binutils/gdb-config.sub.patch",
            "modifies": "gdb-16.3/config.sub"
        },
        {
            "patch": "patches/binutils/gdb-configure.tgt.patch",
            "modifies": "gdb-16.3/gdb/configure.tgt"
        }
    ],
    "bootstrap-commands": [
        [
            "${repo_directory}/gdb-16.3/configure",
                "--prefix=${prefix}",
                "--target=${target_triplet}",
                "--disable-nls",
                "--with-sysroot=${target_prefix}",
                "--disable-werror"
        ]
    ],
    "build-commands": [
        [ "make", "-j${nproc}", "all-gdb" ]
    ],
    "install-commands": [
        [ "make", "-j${nproc}", "install-gdb" ]
    ],
    "host-package": 1,
    "host-provides": "${target_triplet}-gdb"
}
