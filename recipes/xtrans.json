{
    "name": "xtrans",
    "description": "only god knows the description...",
    "version": [ 1,6,0 ],
    "url": "https://www.x.org/archive/individual/lib/xtrans-1.6.0.tar.gz",
    "depends": [ "cross-gcc" ],
    "patches": [],
    "bootstrap-commands": [
        [ "${root_directory}/scripts/do-autoreconf.sh", "${repo_directory}/${name}-${version}" ],
        [ "${repo_directory}/${name}-${version}/configure", "--host=${target_triplet}", "--prefix=/usr" ]
    ],
    "build-commands": [
        [ "make" ]
    ],
    "install-commands": [
        [ "make", "install", "DESTDIR=${prefix}" ],
        [ "make", "install", "DESTDIR=${bin_package_prefix}" ]
    ]
}
