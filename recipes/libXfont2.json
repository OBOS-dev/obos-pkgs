{
    "name": "libXfont2",
    "description": "",
    "version": [ 2,0,6 ],
    "url": "https://www.x.org/archive/individual/lib/libXfont2-2.0.6.tar.gz",
    "depends": [ "cross-gcc", "xorgproto", "libfontenc", "zlib", "xtrans", "freetype" ],
    "patches": [],
    "bootstrap-commands": [
        [ "cp", "${root_directory}/scripts/config.sub", "${repo_directory}/${name}-${version}/config.sub" ],
        [ "${repo_directory}/${name}-${version}/configure", "--host=${target_triplet}", "--prefix=${prefix}", "--disable-malloc0returnsnull", "--disable-static" ]
    ],
    "build-commands": [
        [ "make" ]
    ],
    "install-commands": [
        [ "make", "install" ],
        [ "make", "install", "prefix=${bin_package_prefix}/usr" ]
    ]
}
