{
    "name": "libXfont2",
    "description": "",
    "version": [ 2,0,6 ],
    "url": "https://www.x.org/archive/individual/lib/libXfont2-2.0.6.tar.gz",
    "depends": [ "cross-gcc", "xorgproto", "libfontenc", "zlib", "xtrans", "freetype" ],
    "patches": [],
    "bootstrap-commands": [
        [ "${root_directory}/scripts/do-autoreconf.sh", "${repo_directory}/${name}-${version}" ],
        [ "${repo_directory}/${name}-${version}/configure", "--host=${target_triplet}", "--prefix=/usr", "--disable-malloc0returnsnull", "--disable-static" ]
    ],
    "build-commands": [
        [ "make" ]
    ],
    "install-commands": [
        [ "make", "install", "DESTDIR=${prefix}" ],
        [ "make", "install", "DESTDIR=${bin_package_prefix}" ]
    ]
}
