{
    "name": "tar",
    "description": "GNU Tar",
    "version": [ 1,35,0 ],
    "url": "https://ftpmirror.gnu.org/tar/tar-1.35.tar.xz",
    "depends": [ "cross-gcc" ],
    "patches": [
        {
            "patch": "patches/tar/config.sub.patch",
            "modifies": "tar-1.35/build-aux/config.sub"
        }
    ],
    "bootstrap-commands": [
        [ "${root_directory}/scripts/configure-tar.sh", "${repo_directory}", "${prefix}", "${target_triplet}" ]
    ],
    "build-commands": [
        [ "make", "all" ]
    ],
    "install-commands": [
        [ "make", "install", "DESTDIR=${destdir}" ],
        [ "make", "install", "DESTDIR=${bin_package_prefix}" ]
    ]
}
