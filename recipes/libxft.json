{
    "name": "libxft",
    "description": "idk",
    "url": "https://www.x.org/archive//individual/lib/libXft-2.3.9.tar.xz",
    "version": [ 2,3,9 ],
    "depends": [ "mlibc", "libx11", "libxrender", "libxt", "freetype", "fontconfig" ],
    "build-depends": [ "cross-gcc" ],
    "patches": [],
    "bootstrap-commands": [
        [ "${root_directory}/scripts/do-autoreconf.sh", "${repo_directory}/libXft-${version}" ],
        [ "${repo_directory}/libXft-${version}/configure", "--host=${target_triplet}", "--prefix=${prefix}", "--with-sysroot=${destdir}", "--disable-malloc0returnsnull" ]
    ],
    "build-commands": [
        [ "make", "-j${nproc}" ]
    ],
    "install-commands": [
        [ "make", "-j${nproc}", "install", "DESTDIR=${destdir}" ],
        [ "make", "-j${nproc}", "install", "DESTDIR=${bin_package_prefix}" ]
    ]
}
