{
    "name": "libfontenc",
    "description": "",
    "version": [ 1,1,1 ],
    "url": "https://www.x.org/releases/X11R7.7/src/everything/libfontenc-1.1.1.tar.bz2",
    "depends": [ "cross-gcc", "xorgproto", "zlib" ],
    "patches": [],
    "bootstrap-commands": [
        [ "${root_directory}/scripts/do-autoreconf.sh", "${repo_directory}/${name}-${version}" ],
        [ "${repo_directory}/${name}-${version}/configure", "--host=${target_triplet}", "--prefix=/usr" ]
    ],
    "build-commands": [
        [ "make" ]
    ],
    "install-commands": [
        [ "make", "install", "DESTDIR=${prefix}" ],
        [ "make", "install", "DESTDIR=${bin_package_prefix}" ]
    ]
}
