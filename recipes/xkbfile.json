{
    "name": "xkbfile",
    "description": "xkbfile",
    "version": [ 1,1,3 ],
    "url": "https://www.x.org/archive/individual/lib/libxkbfile-1.1.3.tar.gz",
    "depends": [ "cross-gcc", "xorgproto", "libx11" ],
    "patches": [],
    "bootstrap-commands": [
        [ "${root_directory}/scripts/do-autoreconf.sh", "${repo_directory}/lib${name}-${version}" ],
        [ "${repo_directory}/lib${name}-${version}/configure", "--host=${target_triplet}", "--prefix=${prefix}", "--with-sysroot=${destdir}", "--disable-static", "--enable-shared" ]
    ],
    "build-commands": [
        [ "make", "-j${nproc}" ]
    ],
    "install-commands": [
        [ "make", "install", "DESTDIR=${destdir}" ],
        [ "make", "install", "DESTDIR=${bin_package_prefix}" ]
    ]
}
