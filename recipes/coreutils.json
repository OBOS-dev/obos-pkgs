{
    "name": "coreutils",
    "description": "GNU Coreutils",
    "url": "https://mirror.team-cymru.com/gnu/coreutils/coreutils-9.8.tar.xz",
    "version": [ 9,8,0 ],
    "depends": [ "cross-gcc" ],
    "patches": [],
    "bootstrap-commands": [
        [ "${root_directory}/scripts/do-autoreconf.sh", "${repo_directory}/${name}-9.8" ],
        [ "${root_directory}/scripts/configure-coreutils.sh", "${repo_directory}", "${prefix}", "${target_triplet}" ]
    ],
    "build-commands": [
        [ "make", "all" ]
    ],
    "install-commands": [
        [ "make", "install", "DESTDIR=${destdir}" ],
        [ "make", "install", "DESTDIR=${bin_package_prefix}" ]
    ]
}
