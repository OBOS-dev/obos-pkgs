{
    "name": "freetype",
    "description": "",
    "version": [ 2,12,1 ],
    "url": "https://downloads.sourceforge.net/freetype/freetype-2.12.1.tar.xz",
    "depends": [ "cross-gcc" ],
    "patches": [],
    "bootstrap-commands": [
        [ "cd", "${repo_directory}/${name}-${version}" ],
        [ "./autogen.sh" ],
        [ "cp", "${host_prefix}/share/automake-*/config.sub", "builds/unix/" ],
        [ "cd", "${bootstrap_directory}/${name}" ],
        [ "${repo_directory}/${name}-${version}/configure", "--host=${target_triplet}", "--prefix=/usr", "--enable-shared", "--disable-static", "--with-harfbuzz=no", "--with-librsvg=no", "--with-brotli=no" ]
    ],
    "build-commands": [
        [ "make" ]
    ],
    "install-commands": [
        [ "make", "install", "DESTDIR=${prefix}" ],
        [ "make", "install", "DESTDIR=${bin_package_prefix}" ]
    ]
}
