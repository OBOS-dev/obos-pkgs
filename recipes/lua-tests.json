{
    "name": "lua-tests",
    "description": "Lua Tests",
    "version": [ 5,4,8 ],
    "url": "https://www.lua.org/tests/lua-5.4.8-tests.tar.gz",
    "depends": [],
    "build-depends": [],
    "patches": [],
    "bootstrap-commands": [],
    "build-commands": [],
    "install-commands": [
        [ "mkdir", "-p", "${destdir}/${prefix}/share" ],
        [ "mkdir", "-p", "${destdir}/${prefix}/bin" ],
        [ "mkdir", "-p", "${destdir}/${prefix}/share/lua-tests" ],
        [ "cp", "-r", "${repo_directory}/lua-5.4.8-tests/*.lua", "${destdir}/${prefix}/share/lua-tests" ],
        [ "cp", "-r", "${root_directory}/scripts/run-lua-tests.sh", "${destdir}/${prefix}/bin/run-lua-tests" ],
        [ "mkdir", "-p", "${bin_package_prefix}/${prefix}/share" ],
        [ "mkdir", "-p", "${bin_package_prefix}/${prefix}/bin" ],
        [ "mkdir", "-p", "${bin_package_prefix}/${prefix}/share/lua-tests" ],
        [ "cp", "-r", "${repo_directory}/lua-5.4.8-tests/*.lua", "${bin_package_prefix}/${prefix}/share/lua-tests" ],
        [ "cp", "-r", "${root_directory}/scripts/run-lua-tests.sh", "${bin_package_prefix}/${prefix}/bin/run-lua-tests" ]
    ]
}
