{
    "name": "libx11",
    "description": "",
    "version": [ 1,8,1 ],
    "url": "https://www.x.org/archive/individual/lib/libX11-1.8.1.tar.gz",
    "depends": [ "cross-gcc", "xorgproto", "xtrans", "libxcb>=1.11.1" ],
    "patches": [],
    "bootstrap-commands": [
        [ "cd", "${repo_directory}/libX11-${version}" ],
        [ "autoreconf", "-vfi" ],
        [ "cd", "${bootstrap_directory}/${name}" ],
        [ "echo", "$$PKG_CONFIG_SYSROOT_DIR" ],
        [ "${repo_directory}/libX11-${version}/configure", "--host=${target_triplet}", "--prefix=${prefix}", "--with-sysroot=${prefix}", "--disable-malloc0returnsnull" ]
    ],
    "build-commands": [
        [ "make" ]
    ],
    "install-commands": [
        [ "make", "install", "DESTDIR=${destdir}" ],
        [ "make", "install", "DESTDIR=${bin_package_prefix}" ]
    ]
}
