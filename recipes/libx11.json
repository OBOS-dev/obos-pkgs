{
    "name": "libx11",
    "description": "Unknown",
    "version": [ 1,8,1 ],
    "url": "https://www.x.org/archive/individual/lib/libX11-1.8.1.tar.gz",
    "depends": [ "mlibc", "xtrans", "libxcb>=1.11.1" ],
    "build-depends": [ "cross-gcc", "xorgproto" ],
    "patches": [],
    "bootstrap-commands": [
        [ "cd", "${repo_directory}/libX11-${version}" ],
        [ "autoreconf", "-vfi" ],
        [ "cd", "${bootstrap_directory}/${name}" ],
        [ "${repo_directory}/libX11-${version}/configure", "--host=${target_triplet}", "--prefix=${prefix}", "--with-sysroot=${destdir}", "--disable-malloc0returnsnull" ]
    ],
    "build-commands": [
        [ "make" ]
    ],
    "install-commands": [
        [ "make", "install", "DESTDIR=${destdir}" ],
        [ "make", "install", "DESTDIR=${bin_package_prefix}" ],
        [ "rm", "${bin_package_prefix}/usr/include/X11/extensions/XKBgeom.h" ]
    ]
}
