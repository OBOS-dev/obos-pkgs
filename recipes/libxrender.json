{
    "name": "libxrender",
    "description": "...",
    "version": [ 0,9,12 ],
    "url": "https://www.x.org/releases/individual/lib/libXrender-0.9.12.tar.xz",
    "depends": [ "mlibc", "libx11" ],
    "build-depends": [ "cross-gcc", "xorgproto" ],
    "patches": [],
    "bootstrap-commands": [
        [ "${root_directory}/scripts/do-autoreconf.sh", "${repo_directory}/libXrender-${version}" ],
        [ "${repo_directory}/libXrender-${version}/configure", "--host=${target_triplet}", "--prefix=${prefix}", "--disable-static", "--enable-shared", "--disable-malloc0returnsnull" ]
    ],
    "build-commands": [
        [ "make" ]
    ],
    "install-commands": [
        [ "make", "install", "DESTDIR=${destdir}" ],
        [ "make", "install", "DESTDIR=${bin_package_prefix}" ]
    ]
}
