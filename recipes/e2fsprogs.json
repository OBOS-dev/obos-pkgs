{
    "name": "e2fsprogs",
    "description": "EXT2 Filesystem Programs",
    "version": [ 1,47,3 ],
    "depends": [ "cross-gcc" ],
    "git-url": "https://git.kernel.org/pub/scm/fs/ext2/e2fsprogs",
    "git-commit": "v1.47.3",
    "patches": [
        {
            "patch": "patches/e2fsprogs/config.sub.patch",
            "modifies": "e2fsprogs/config/config.sub"
        },
        {
            "patch": "patches/e2fsprogs/create_inode.c.patch",
            "modifies": "e2fsprogs/misc/create_inode.c"
        },
        {
            "patch": "patches/e2fsprogs/debugfs.c.patch",
            "modifies": "e2fsprogs/debugfs/debugfs.c"
        },
        {
            "patch": "patches/e2fsprogs/devname.c.patch",
            "modifies": "e2fsprogs/lib/blkid/devname.c"
        },
        {
            "patch": "patches/e2fsprogs/getsize.c.patch",
            "modifies": "e2fsprogs/lib/blkid/getsize.c"
        }
    ],
    "bootstrap-commands": [
        [ "${repo_directory}/e2fsprogs/configure", "--host=${target_triplet}", "--prefix=${prefix}" ]
    ],
    "build-commands": [
        [ "make", "all" ]
    ],
    "install-commands": [
        [ "make", "install", "DESTDIR=${destdir}" ],
        [ "make", "install", "DESTDIR=${bin_package_prefix}" ]
    ]
}
