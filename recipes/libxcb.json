{
    "name": "libxcb",
    "description": "",
    "version": [ 1,16,0 ],
    "url": "https://www.x.org/archive/individual/lib/libxcb-1.16.tar.gz",
    "depends": [ "cross-gcc", "xorgproto", "libXdmcp", "libXau", "xcb-proto", "libpthread-stubs" ],
    "patches": [],
    "bootstrap-commands": [
        [ "${root_directory}/scripts/do-autoreconf.sh", "${repo_directory}/${name}-1.16" ],
        [ "${repo_directory}/${name}-1.16/configure", "--host=${target_triplet}", "--prefix=${prefix}", "--disable-static", "--with-sysroot=${destdir}" ]
    ],
    "build-commands": [
        [ "make" ]
    ],
    "install-commands": [
        [ "make", "install", "DESTDIR=${destdir}" ],
        [ "make", "install", "DESTDIR=${bin_package_prefix}" ]
    ]
}
