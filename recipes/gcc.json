{
    "name": "gcc",
    "description": "Compiler used to compile",
    "depends": [ "cross-gcc", "binutils", "libgmp", "libmpfr", "libmpc" ],
    "patches": [],
    "bootstrap-commands": [
        [ "${repo_directory}/gcc-14.2.0/configure",
                "--with-build-sysroot=${prefix}",
                "--target=${target_triplet}",
                "--prefix=${prefix}",
                "--with-sysroot=/",
                "--disable-nls", "--enable-languages=c,c++",
                "--disable-shared",
                "--disable-multilib",
                "--host=${target_triplet}",
                "--enable-threads=posix"
        ]
    ],
    "build-commands": [
        [ "make", "-j${nproc}", "all-gcc" ],
        [ "make", "-j${nproc}", "all-target-libgcc" ]
    ],
    "install-commands": [
        [ "make", "-j${nproc}", "install-gcc" ],
        [ "make", "-j${nproc}", "install-target-libgcc" ]
        [ "make", "-j${nproc}", "prefix=${bin_package_prefix}", "install-gcc" ],
        [ "make", "-j${nproc}", "prefix=${bin_package_prefix}", "install-target-libgcc" ]
    ]
}
