{
    "name": "gcc",
    "description": "Compiler used to compile",
    "version": [ 14,2,0 ],
    "depends": [ "mlibc", "binutils>=2.35.0", "libgmp>=4.3.2", "libmpfr>=3.1.0", "libmpc>=1.0.1" ],
    "build-depends": [ "cross-gcc>=14.2.0" ],
    "patches": [],
    "bootstrap-commands": [
        [ "${repo_directory}/gcc-14.2.0/configure",
                "--target=${target_triplet}",
                "--prefix=${prefix}",
                "--with-sysroot=${prefix}",
                "--with-build-sysroot=${destdir}",
                "--disable-nls", "--enable-languages=c,c++",
                "--disable-shared",
                "--disable-multilib",
                "--host=${target_triplet}",
                "--enable-threads=posix"
        ]
    ],
    "build-commands": [
        [ "make", "-j${nproc}", "all-gcc" ],
        [ "make", "-j${nproc}", "all-target-libgcc" ]
    ],
    "install-commands": [
        [ "make", "-j${nproc}", "install-strip-gcc", "DESTDIR=${destdir}" ],
        [ "make", "-j${nproc}", "install-strip-gcc", "DESTDIR=${bin_package_prefix}" ],
        [ "make", "-j${nproc}", "install-strip-target-libgcc", "DESTDIR=${destdir}" ],
        [ "make", "-j${nproc}", "install-strip-target-libgcc", "DESTDIR=${bin_package_prefix}" ]
    ],
    "inhibit-auto-rebuild": true
}
