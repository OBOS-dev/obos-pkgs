{
    "name": "xkbcomp",
    "description": "whydoievenbotherwithdescribingthis?",
    "version": [ 1,4,7 ],
    "url": "https://www.x.org/archive/individual/app/xkbcomp-1.4.7.tar.gz",
    "depends": [ "cross-gcc", "libxcb", "xcb-proto", "xkbfile" ],
    "patches": [],
    "bootstrap-commands": [
        [ "${root_directory}/scripts/do-autoreconf.sh", "${repo_directory}/${name}-${version}" ],
        [ "${repo_directory}/${name}-${version}/configure", "--host=${target_triplet}", "--prefix=${prefix}" ]
    ],
    "build-commands": [
        [ "make" ]
    ],
    "install-commands": [
        [ "make", "install", "DESTDIR=${destdir}" ],
        [ "make", "install", "DESTDIR=${bin_package_prefix}" ]
    ]
}
