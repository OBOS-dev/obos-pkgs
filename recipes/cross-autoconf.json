{
    "name": "cross-autoconf",
    "description": "Cross autoconf",
    "version": [ 2,72,0 ],
    "url": "https://ftpmirror.gnu.org/gnu/autoconf/autoconf-2.72.tar.gz",
    "depends": [ "cross-m4", "cross-autoconf-archive" ],
    "build-depends": [],
    "patches": [
        {
            "patch": "patches/autoconf/config.sub.patch",
            "modifies": "autoconf-2.72/build-aux/config.sub"
        },
        {
            "patch": "patches/autoconf/config.guess.patch",
            "modifies": "autoconf-2.72/build-aux/config.guess"
        }
    ],
    "bootstrap-commands": [
        [ "${repo_directory}/autoconf-2.72/configure", "--prefix", "${prefix}" ]
    ],
    "build-commands": [
        [ "make", "all" ]
    ],
    "install-commands": [
        [ "make", "install" ]
    ],
    "host-package": true
}
