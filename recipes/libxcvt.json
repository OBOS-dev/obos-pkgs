{
    "name": "libxcvt",
    "description": "only god knows the description...",
    "version": [ 0,1,2 ],
    "url": "https://www.x.org/pub/individual/lib/libxcvt-0.1.2.tar.xz",
    "depends": [ "mlibc" ],
    "build-depends": [ "cross-gcc", "meson-crossfile" ],
    "patches": [],
    "bootstrap-commands": [
        [ "meson", "setup", "--cross-file=${bootstrap_directory}/meson-crossfile.txt", "--prefix=${prefix}", "${repo_directory}/${name}-${version}" ]
    ],
    "build-commands": [
        [ "meson", "compile" ]
    ],
    "install-commands": [
        [ "meson", "install", "--destdir=${destdir}" ],
        [ "meson", "install", "--destdir=${bin_package_prefix}" ]
    ]
}
