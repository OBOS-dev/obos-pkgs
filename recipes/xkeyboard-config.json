{
    "name": "xkeyboard-config",
    "description": "Xorg Keyboard Configuration, probably",
    "version": [ 2,36,0 ],
    "url": "http://www.x.org/releases/individual/data/xkeyboard-config/xkeyboard-config-2.36.tar.xz",
    "depends": [ "cross-gcc", "meson-crossfile" ],
    "patches": [],
    "bootstrap-commands": [
        [ "meson", "setup", "--cross-file=${bootstrap_directory}/meson-crossfile.txt", "--prefix=${prefix}", "${repo_directory}/${name}-2.36" ]
    ],
    "build-commands": [
        [ "meson", "compile" ]
    ],
    "install-commands": [
        [ "meson", "install", "--destdir=${destdir}" ],
        [ "meson", "install", "--destdir=${bin_package_prefix}" ]
    ]
}
