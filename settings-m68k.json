{
    "cross-compile": 1,
    "target-triplet": "m68k-obos",
    "binary-packages-default": 1,
    "environment": [
        {
            "env": "ACLOCAL_PATH",
            "value": "${host_prefix}/share/aclocal:/usr/share/aclocal/",
            "replace": false
        },
        {
            "env": "PKG_CONFIG_PATH",
            "value": "${destdir}/usr/lib/pkgconfig:${destdir}/usr/share/pkgconfig",
            "replace": true
        },
        {
            "env": "PKG_CONFIG_SYSROOT_DIR",
            "value": "${destdir}",
            "replace": true
        },
        {
            "env": "LD_LIBRARY_PATH",
            "value": "${host_prefix}/lib",
            "replace": true
        }
    ]
}
