{
    "cross-compile": 1,
    "target-triplet": "m68k-obos",
    "binary-packages-default": 1,
    "environment": [
        {
            "env": "ACLOCAL_PATH",
            "value": "${destdir}/share/aclocal:/usr/share/aclocal/",
            "replace": false
        },
        {
            "env": "PKG_CONFIG_PATH",
            "value": "${destdir}/lib/pkgconfig:${destdir}/share/pkgconfig",
            "replace": true
        },
        {
            "env": "PKG_CONFIG_SYSROOT_DIR",
            "value": "${destdir}",
            "replace": true
        }
    ]
}
