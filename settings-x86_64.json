{
    "cross-compile": 1,
    "target-triplet": "x86_64-obos",
    "binary-packages-default": 1,
    "environment": [
        {
            "env": "ACLOCAL_PATH",
            "value": "${host_prefix}/share/aclocal:/usr/share/aclocal/",
            "replace": false
        },
        {
            "env": "PKG_CONFIG_PATH",
            "value": "${destdir}/usr/lib/pkgconfig:${destdir}/usr/share/pkgconfig",
            "replace": true
        },
        {
            "env": "PKG_CONFIG_SYSROOT_DIR",
            "value": "${destdir}",
            "replace": true
        },
        {
            "env": "OVMF",
            "value": "/usr/share/OVMF/OVMF_CODE_4M.fd",
            "replace": false
        },
        {
            "env": "OBOS_USE_LIMINE",
            "value": "OFF",
            "replace": false
        }
    ]
}
