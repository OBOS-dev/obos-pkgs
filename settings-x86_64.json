{
    "cross-compile": 1,
    "target-triplet": "x86_64-obos",
    "binary-packages-default": 1,
    "environment": [
        {
            "env": "ACLOCAL_PATH",
            "value": "/usr/share/aclocal/",
            "replace": false
        },
        {
            "env": "PKG_CONFIG_PATH",
            "value": "${target_prefix}/lib/pkgconfig:${target_prefix}/share/pkgconfig",
            "replace": true
        },
        {
            "env": "PKG_CONFIG_SYSROOT_DIR",
            "value": "${target_prefix}",
            "replace": true
        }
    ]
}
